--SQL学習--
SELECT * FROM nobel
 WHERE winner IN ('Theodore Roosevelt',
                  'Woodrow Wilson',
                  'Jimmy Carter',
                  "Barack Obama");


SELECT winner
FROM nobel
WHERE winner 
LIKE "John%";


SELECT yr, subject, winner
FROM nobel
WHERE subject = "physics"
AND yr = 1980
OR subject = "chemistry"
AND yr = 1984
;



SELECT *
FROM nobel
WHERE yr = 1980
AND subject <> "chemistry"
AND subject <> "medicine"
;

