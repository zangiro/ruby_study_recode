import java.util.Scanner;
 
class Main {
  public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
    int num = Integer.parseInt(sc.nextLine());
    int count = 1;
    int total = num;
    while (total < 24) {
      total += num;
      count++;
    }
    System.out.println(count);
  }
}

