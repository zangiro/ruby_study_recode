import java.util.Scanner;
 
class Main {
  public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
    String split_word [] = sc.nextLine().split(" ");
    int requited_time = Integer.parseInt(split_word[0]);
    int minute = Integer.parseInt(split_word[1]) * 60;
    
    System.out.println(minute / requited_time);
  }
}

