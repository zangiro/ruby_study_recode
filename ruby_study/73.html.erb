import java.util.Scanner;
 
class Main {
  public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
    String humans = sc.nextLine();
    String split_result [] = sc.nextLine().split(" ");
    int pah_result = 0;
    int rock_result = 0;
    for (int i = 0; i < split_result.length; i++) {
      if (split_result[i].equals("G")) {
        int pah_result += 1
      } else {
        int rock_result += 1
      }
    }
    if (pah_result > rock_result) {
      System.out.println("P");
    } else (pah_result < rock_result) {
      System.out.println("G");
    } else {
      System.out.println("Draw");
    }
  }
}

