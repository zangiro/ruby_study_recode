import java.util.Scanner;
 
class Main {
  public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
    char char_word [] = sc.nextLine().toCharArray();
    int count = 0;
    for (int i = 0; i < char_word.length; i++) {
      if (char_word[i] == 'A') {
        count += 1;
      }
    }
    System.out.println(count);
  }
}
