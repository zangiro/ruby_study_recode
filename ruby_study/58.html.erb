lines = $stdin.readlines.map{|word|word.chomp}
minimum_cost = lines[0].to_i
how_item = lines[1].to_i
array = lines[2..(how_item + 1)].map{|a|a.to_i}.sort
# [1, 2, 3...


count = 0
how_many_time = 1

(how_item).times do
  array.combination(how_many_time).each do |combo|
    min_number = combo.min
    sum_number = combo.sum
    if (sum_number - min_number) < minimum_cost && sum_number >= minimum_cost
      count += 1
    end
  end
  how_many_time += 1
end