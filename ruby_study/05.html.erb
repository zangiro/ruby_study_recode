lines = $stdin.readlines.map{|word|word.chomp}
high = lines[0].split[0].to_i
width = lines[0].split[1].to_i
how_many = lines[1].split[0].to_i
attack = lines[1].split[1].to_i
array = lines[2..(how_many+1)].map{|a|a.split.map{|b|b.to_i}}
# [[1, 2], [1, 3], [1, 2], [2, 1], [1, 3]]
record = Array.new(high) { Array.new(width, 0) }

array.each do |arr| #[1, 2] . . . . . .
  x = arr[0] - 1
  y = arr[1] - 1
  record[x][y] += 1
end

conversion_record = record.flatten
sort = conversion_record.sort.reverse

if attack > sort.length
  attack = sort.length
end
index = 0
count = 0

attack.times do
  count += sort[index]
  index += 1
end
p count