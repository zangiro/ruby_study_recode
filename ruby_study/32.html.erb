lines = $stdin.readlines.map{|word|word.chomp}
cards = lines[0].to_i
numbers = lines[1].split.map{|a|a.to_i}
answer = -Float::INFINITY

(0...cards).each do |first|
  (0...cards).each do |second|
    next if second == first
    total = nil
    (0...cards).each do |num|
      if total == nil && num == first
        total = numbers[num] + 1
      elsif total == nil
        total = numbers[num]
      elsif num == first
        total *= (numbers[num] + 1)
      elsif num == second
        total *= (numbers[num] - 1)
      else
        total *= numbers[num]
      end
    end

    if answer < total
      answer = total
    end
  end
end









