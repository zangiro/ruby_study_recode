パーフェクトシャッフル問題
lines = $stdin.readlines.map{|a|a.chomp}
number = lines[0].split[0].to_i
how_many = lines[0].split[1].to_i
array = (1..number).to_a
# [1, 2, 3, 4, 5, 6, 7, 8]




new_array = []

how_many.times do
  if new_array == []
    division_under = array.take(array.length / 2)
    # [1, 2, 3, 4]
    division_over = array.drop(array.length / 2)
    # [5, 6, 7, 8]
  else
    division_under = new_array.take(array.length / 2)
    division_over = new_array.drop(array.length / 2)
    new_array = []
  end

  index = 0
  (division_under.length).times do
    new_array << division_over[index]
    new_array << division_under[index]
    index += 1
  end
end

answer = new_array.join(" ")